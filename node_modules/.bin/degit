#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/b/Nextcloud/CODE/proj/kessel-cli/node_modules/.pnpm/degit@2.8.4/node_modules/degit/node_modules:/proc/cygdrive/b/Nextcloud/CODE/proj/kessel-cli/node_modules/.pnpm/degit@2.8.4/node_modules:/proc/cygdrive/b/Nextcloud/CODE/proj/kessel-cli/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/b/Nextcloud/CODE/proj/kessel-cli/node_modules/.pnpm/degit@2.8.4/node_modules/degit/node_modules:/proc/cygdrive/b/Nextcloud/CODE/proj/kessel-cli/node_modules/.pnpm/degit@2.8.4/node_modules:/proc/cygdrive/b/Nextcloud/CODE/proj/kessel-cli/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/degit@2.8.4/node_modules/degit/degit" "$@"
else
  exec node  "$basedir/../.pnpm/degit@2.8.4/node_modules/degit/degit" "$@"
fi
