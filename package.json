{
  "name": "kessel-cli",
  "version": "2.1.0",
  "description": "CLI für die Kessel Boilerplate - Erstellt neue Next.js-Projekte mit Supabase & ShadCN UI",
  "type": "module",
  "main": "dist/cli.mjs",
  "bin": {
    "kessel": "dist/cli.mjs"
  },
  "scripts": {
    "build": "tsup && node scripts/add-shebang.mjs",
    "dev": "tsup --watch",
    "start": "node dist/cli.mjs",
    "kessel": "node dist/cli.mjs",
    "test": "node tests/run-all.mjs",
    "test:all": "node tests/run-all.mjs",
    "test:profile": "node tests/profile.test.mjs",
    "test:infra-dev": "node tests/infra-dev-db.test.mjs",
    "test:framework": "node tests/test-framework.mjs",
    "test:project": "node tests/test-project-creation.mjs",
    "test:validation": "node tests/url-jwt-validation.test.mjs"
  },
  "keywords": [
    "nextjs",
    "supabase",
    "shadcn",
    "cli",
    "boilerplate"
  ],
  "author": "Philip König",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/phkoenig/kessel-cli.git"
  },
  "files": [
    "dist/",
    "lib/",
    "config.example.json",
    "README.md"
  ],
  "packageManager": "pnpm@10.22.0",
  "dependencies": {
    "@supabase/supabase-js": "^2.81.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "degit": "^2.8.4",
    "enquirer": "^2.4.1",
    "figlet": "^1.9.4",
    "ink": "^6.6.0",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-select-input": "^6.2.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "6.0.0",
    "inquirer": "^12.11.1",
    "listr2": "^8.0.0",
    "octokit": "^5.0.5",
    "pg": "^8.11.0",
    "react": "^19.2.3"
  },
  "devDependencies": {
    "@types/figlet": "^1.7.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
